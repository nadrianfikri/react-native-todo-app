{
  "expo": {
    "name": "Todo App",
    "slug": "nad-todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/todo-icon.png",
    "splash": {
      "image": "./assets/todo-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#161721"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dumbways.nadtodoapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.dumbways.nadtodoapp",
      "versionCode": 1,
      "enableDangerousExperimentalLeanBuilds": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/todo-icon.png",
        "backgroundColor": "#161721"
      }
    },
    "web": {
      "favicon": "./assets/todo-icon.png"
    }
  }
}
